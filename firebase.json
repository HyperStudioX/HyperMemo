{
    "functions": [
        {
            "source": "functions",
            "codebase": "default",
            "disallowLegacyRuntimeConfig": true,
            "ignore": [
                "venv",
                "__pycache__",
                "*.pyc",
                ".git",
                "firestore-debug.log",
                "firebase-debug.log",
                "firebase-debug.*.log",
                "*.local"
            ],
            "runtime": "python313",
            "endpoints": {
                "bookmarks": {
                    "entryPoint": "bookmarks",
                    "availableMemoryMb": 512,
                    "httpsTrigger": {
                        "invoker": ["allUsers"]
                    }
                },
                "summaries": {
                    "entryPoint": "summaries",
                    "availableMemoryMb": 512,
                    "httpsTrigger": {
                        "invoker": ["allUsers"]
                    }
                },
                "summary_tags": {
                    "entryPoint": "summary_tags",
                    "availableMemoryMb": 512,
                    "httpsTrigger": {
                        "invoker": ["allUsers"]
                    }
                },
                "rag_query": {
                    "entryPoint": "rag_query",
                    "availableMemoryMb": 512,
                    "httpsTrigger": {
                        "invoker": ["allUsers"]
                    }
                }
            }
        }
    ]
}
